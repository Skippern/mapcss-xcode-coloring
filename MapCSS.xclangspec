		BasedOn = "xcode.lang.simpleColoring";
		IncludeInMenu = YES;
		Name = MapCSS;
		Syntax = {
			Tokenizer = "xcode.lang.mapcss.lexer";
			IncludeRules = (
				"xcode.lang.mapcss.style",
				"xcode.lang.mapcss.block",
			);
			Type = "xcode.syntax.plain";
		};
