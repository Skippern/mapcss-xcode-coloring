	{
		Identifier = "xcode.lang.mapcss.hexcolor";
		Syntax = {
			StartChars = "#";
			Chars = "0123456789abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ";
			Match = (
				"^#[0-9a-fA-F]([0-9a-fA-F])?%?",
			);
			Type = "xcode.syntax.number";
		};
	},
	{
		Identifier = "xcode.lang.mapcss.number";
		Syntax = {
			StartChars = "0123456789-+.";
			Chars = "0123456789.%";
			Match = (
				"^[-+]?[0-9]+(\\.[0-9]+)?%?",
				"^[-+]?\\.[0-9]+%?",
			);
			Type = "xcode.syntax.number";
		};
